{
  "name": "simialbi/yii2-facebook-graph-sdk",
  "description": "yii2 facebook api client component",
  "keywords": [
    "yii2",
    "facebook",
    "facebook api",
    "api",
    "sdk",
    "graph"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/simialbi/yii2-facebook-graph-sdk/issues",
    "source": "https://github.com/simialbi/yii2-facebook-graph-sdk"
  },
  "authors": [
    {
      "name": "Simon Karlen",
      "email": "simi.albi@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=5.6",
    "yiisoft/yii2": "^2.0.0",
    "simialbi/yii2-simialbi-base": "*",
    "facebook/graph-sdk": "^5.7.0"
  },
  "require-dev": {
    "yiisoft/yii2-coding-standards": "~2.0",
    "phpunit/phpunit": "*",
    "facebook/php-business-sdk": "^3.2.6"
  },
  "suggest": {
    "facebook/php-business-sdk": "Use facebook business sdk"
  },
  "config": {
    "process-timeout": 1800
  },
  "autoload": {
    "psr-4": {
      "simialbi\\yii2\\facebook\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "yiiunit\\extension\\facebook\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}